			&lt;th _ngcontent-c15=&quot;&quot; style=&quot;width: 347px; text-align: left;&quot;&gt;&lt;span style=&quot;line-height:1;&quot;&gt;&lt;span style=&quot;font-size:14px;&quot;&gt;RST&lt;/span&gt;&lt;/span&gt;&lt;/th&gt;
