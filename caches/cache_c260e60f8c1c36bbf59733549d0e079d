";s:5:"tenen";s:57:"Accossato Billet Brake Master Cylinder 15x20 Folder Lever";s:5:"tenvi";s:34:"Tay Thắng Accossato Billet 15X20";s:4:"masp";s:11:"CY072L20RST";s:3:"gia";s:7:"7050000";s:5:"giakm";s:1:"0";s:6:"giamoi";s:1:"0";s:5:"giado";s:5:"233.5";s:8:"giadomoi";s:1:"0";s:3:"stt";s:2:"35";s:7:"hienthi";s:1:"1";s:4:"type";s:8:"san-pham";s:7:"ngaytao";s:10:"1695627834";s:7:"ngaysua";s:10:"1721112925";s:7:"luotxem";s:3:"275";s:10:"motanganvi";s:0:"";s:10:"motanganen";s:0:"";s:7:"banchay";s:1:"0";s:4:"sale";s:1:"0";s:7:"giasize";s:0:"";s:6:"taptin";s:0:"";s:8:"cothemua";s:1:"2";s:10:"soluongkho";s:1:"0";s:9:"ungdungvi";s:163:"&lt;p&gt; &lt;/p&gt;

&lt;div class=&quot;eJOY__extension_root_class&quot; id=&quot;eJOY__extension_root&quot; style=&quot;all: unset;&quot;&gt; &lt;/div&gt;
